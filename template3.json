{
	"metodo": "POST",
	"base_url": "https://api.exemplo.com",
	"endpoint": "/user/{user_id}/pedidos",
	"headers": {
		"auth": "Bearer {token}",
		"Content-Type": "application/json"
	},    
    "query": {
		"pedido_id": "str",
		"shop": "str"
	},	
	"body": {
        "date": "str",
		"cpf": "str",
        "pedido": {
            "details": {
                "product_id": "str",
                "quantity": 0
            }
        }	
	}
}